<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>InsertData</servlet-name>
        <servlet-class>InsertData</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DiaplayRecord</servlet-name>
        <servlet-class>DisplayRecord</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditRecord</servlet-name>
        <servlet-class>EditRecord</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>InsertData</servlet-name>
        <url-pattern>/InsertData</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DiaplayRecord</servlet-name>
        <url-pattern>/DiaplayRecord</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditRecord</servlet-name>
        <url-pattern>/EditRecord</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
